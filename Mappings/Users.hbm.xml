<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="ChatApp"
                   namespace="ChatApp.Models">

	<class name="User">
		<id name="UserId">
			<generator class="int" />
		</id>
		<property name="UserName" />
		<property name="FirstName" />
		<property name="LastName" />
		<property name="DateCreated" />
	</class>
</hibernate-mapping>