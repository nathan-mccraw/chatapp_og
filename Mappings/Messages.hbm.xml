<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="ChatApp"
                   namespace="ChatApp.Models">

	<class name="Message">
		<id name="MessageId">
			<generator class="int" />
		</id>
		<property name="Text" />
		<property name="UserId" />
		<property name="UserName" />
		<property name="DateCreated" />
	</class>
</hibernate-mapping>